{
  "name": "mcp-server-3gpp",
  "version": "1.0.0",
  "description": "MCP Server for searching 3GPP specification documents - NAS, RF, RCT, Protocol, IMS, RSE specs for NAD team",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "mcp-server-3gpp": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "postinstall": "node scripts/postinstall.js",
    "download-pdfs": "node scripts/download-pdfs.js",
    "prepare-data": "node scripts/prepare-data.js",
    "setup": "npm run download-pdfs && npm run prepare-data"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "3gpp",
    "lte",
    "5g",
    "nr",
    "nas",
    "emm",
    "5gmm",
    "rf",
    "rct",
    "ims",
    "rse",
    "protocol",
    "usim",
    "usat",
    "nad",
    "telecom",
    "ai",
    "copilot"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lee-SiHyeon/mcp-server-3gpp.git"
  },
  "homepage": "https://github.com/Lee-SiHyeon/mcp-server-3gpp#readme",
  "bugs": {
    "url": "https://github.com/Lee-SiHyeon/mcp-server-3gpp/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^4.1.13"
  }
}
